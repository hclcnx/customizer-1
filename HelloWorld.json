{
  "name": "DLS Homepage Customization",
  "title": "Introductory Customizer App",
  "description": "Inserts text into IBM Connections Homepage",
  "services": [
    "Customizer"
  ],
  "extensions": [
    {
      "payload": {
	      "match": {
		      "user name":["Lucille Suarez"]
	      },
        "include-files": [
          "helloWorld.user.js"
        ],
	      "include-repo":{
	       "name": "customizer-1"
	     }
      },
      "name": "DLS Hello World Extension",
      "type": "com.ibm.customizer.ui",
      "path": "homepage"
    }
  ]
}
